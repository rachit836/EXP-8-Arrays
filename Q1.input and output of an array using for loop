#include<iostream>
using namespace std;
int main(){
    cout<<"enter the size of array : ";
    int n;
    cin>>n;
    int numbers[n];
    cout<<"enter the values "<<endl;
    for(int i=0;i<n;i++){
        
        cin>>numbers[i];
        
    }
    cout<<"the values are "<<endl;
    for(int i=0;i<n;i++){
        cout<<numbers[i]<<endl;
    }
    return 0;
}

OUTPUT:
enter the size of array : 5
enter the values 
10
20
30
40
50
the values are 
10
20
30
40
50
