#include<iostream>
using namespace std;
int main(){
    string str1;
    cout<<"enter the string 1 = ";
    cin>>str1;
    string str2;
    cout<<"entee the string 2 = ";
    cin>>str2;
    string str3=str1+" "+str2;
    cout<<str3<<endl;
    int n=str3.length();
    cout<<"the lenght of the function is = "<<n<<endl;
    
    return 0;
}
output:
enter the string 1 = rachit
enter the string 2 = singh
