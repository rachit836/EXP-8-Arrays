#include<iostream>
using namespace std;
int main(){
    cout<<"enter the size of array : ";
    int n;
    cin>>n;
    int numbers[n];
    cout<<"enter the values "<<endl;
    for(int i=0;i<n;i++){
       
        cin>>numbers[i];
        
    }
    cout<<"the reversed array is : "<<endl;
    for(int j=n-1;j>=0;j--){
    
        
        cout<<numbers[j]<<endl;
    }
    
   
       
    
    return 0;
}

OUTPUT:
enter the size of array : 5
enter the values 
15

25
30
35
45
the reversed array is : 
45
35
30
25
15


